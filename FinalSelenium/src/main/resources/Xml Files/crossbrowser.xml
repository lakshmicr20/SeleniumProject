<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="crossbrowser" thread-count="3" parallel="tests">
<listeners>
<listener class-name="com.supermarket.listeners.ExtentReporter"></listener>
</listeners>
<test name="reggressionchrome">
<parameter name="browser" value="chrome"></parameter>
<classes>
<class name="com.FinalSelenium.tests.LoginTest"></class>
<class name="com.FinalSelenium.tests.AdminUserTest"></class>
</classes>
</test>
<test name="reggressionedge">
<parameter name="browser" value="edge"></parameter>
<classes>
<class name="com.FinalSelenium.tests.LoginTest"></class>
<class name="com.FinalSelenium.tests.AdminUserTest"></class>
</classes>
</test>
<test name="reggressionfirefox">
<parameter name="browser" value="firefox"></parameter>
<classes>
<class name="com.FinalSelenium.tests.LoginTest"></class>
<class name="com.FinalSelenium.tests.AdminUserTest"></class>
</classes>
</test>
</suite>
